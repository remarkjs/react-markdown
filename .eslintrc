{
  "extends": ["sanity", "prettier", "plugin:react/recommended"],
  "plugins": ["react"],
  "env": {"node": true, "browser": true},
  "parserOptions": {"ecmaVersion": 2018, "ecmaFeatures": {"jsx": true}},
  "rules": {"strict": 0, "complexity": ["warn", 15]},
  "settings": {
    "react": {
      "version": "15.0"
    }
  }
}
