    "react-is": "^17.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "space-separated-tokens": "^1.1.0",
    "style-to-object": "^0.3.0",
    "unified": "^9.0.0",
    "unist-util-is": "^4.1.0",
    "unist-util-visit": "^2.0.0"
  },
